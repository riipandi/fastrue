{
    "name": "fastrue",
    "private": true,
    "scripts": {
        "clean": "turbo run clean",
        "dev": "turbo run dev --filter=!website",
        "build": "turbo run build --filter=!website",
        "start": "turbo run start --filter=!website",
        "deps": "pnpm dlx npm-check-updates -x typescript --deep --interactive",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "preinstall": "npx only-allow pnpm",
        "lint": "turbo run lint",
        "test": "turbo run test"
    },
    "devDependencies": {
        "eslint-config-turbo": "^1.10.15",
        "eslint": "^8.51.0",
        "prettier": "^3.0.3",
        "rimraf": "^5.0.5",
        "turbo": "^1.10.15"
    },
    "packageManager": "pnpm@8.8.0"
}
